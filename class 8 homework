def is_pangram(text):
    alphabet = set("abcdefghijklmnopqrstuvwxyz")
    return alphabet <= set(text.lower())

# Ask user for input
user_input = input("Enter a string: ")

if is_pangram(user_input):
    print("Yes, the string is a pangram.")
else:
    print("No, the string is not a pangram.")